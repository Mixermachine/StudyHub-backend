    "helmet": "3.11.0",
    "jsonwebtoken": "8.1.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.11",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.0.9",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "1.14.12"
  }
