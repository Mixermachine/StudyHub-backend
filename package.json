{
  "name": "studyhub-backend",
  "version": "1.0.0",
  "description": "Studyhub backend as a service",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Mixermachine/StudyHub-backend.git"
  },
  "author": "StudyHub Team",
  "license": "MIT",
